"student":[
  {% for student in students %}
    {% for contact in contacts %}
      {% for place in placework %}
        {"id": "{{ student.id }}",
        "name": "{{ student.last_name }} {{ student.first_name }} {{ student.second_name }}",
        "telephone": "{{ contact.telephone }}",
        "email": "{{ contact.email }}",
        "placework": "{{ place.departament }}"
        },
      {% endfor %}
    {% endfor %}
  {% endfor %}
]
